{
  "files": [],
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "preserveSymlinks": true,
    "outDir": "dist",
    "plugins": [],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "composite": true,
    "paths": {
      "modules/*": [
        "./*/src"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "reflection": true,
  "references": [
    {
      "path": "./modules/dash/tsconfig.cjs.json"
    },
    {
      "path": "./modules/deps/tsconfig.cjs.json"
    },
    {
      "path": "./modules/entities/tsconfig.cjs.json"
    },
    {
      "path": "./modules/errors/tsconfig.cjs.json"
    },
    {
      "path": "./modules/files/tsconfig.cjs.json"
    },
    {
      "path": "./modules/files-client/tsconfig.cjs.json"
    },
    {
      "path": "./modules/format-terminal/tsconfig.cjs.json"
    },
    {
      "path": "./modules/profile/tsconfig.cjs.json"
    },
    {
      "path": "./modules/rpc-ws/tsconfig.cjs.json"
    },
    {
      "path": "./modules/schema/tsconfig.cjs.json"
    },
    {
      "path": "./modules/transport/tsconfig.cjs.json"
    },
    {
      "path": "./modules/types/tsconfig.cjs.json"
    }
  ]
}
