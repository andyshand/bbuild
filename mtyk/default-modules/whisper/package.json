{
  "name": "@bbuild/whisper",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./_deps": {
      "import": "./_deps.js",
      "require": "./dist/cjs/_deps.js"
    },
    "./audio-converter": {
      "import": "./audio-converter.js",
      "require": "./dist/cjs/audio-converter.js"
    },
    "./index": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./model-service": {
      "import": "./model-service.js",
      "require": "./dist/cjs/model-service.js"
    },
    "./model-size": {
      "import": "./model-size.js",
      "require": "./dist/cjs/model-size.js"
    },
    "./response": {
      "import": "./response.js",
      "require": "./dist/cjs/response.js"
    },
    "./server": {
      "import": "./server.js",
      "require": "./dist/cjs/server.js"
    },
    "./types/*": {
      "import": "./types/*.js",
      "require": "./dist/cjs/types/*.js"
    },
    "./types": {
      "import": "./types/index.js",
      "require": "./dist/cjs/types/index.js"
    },
    "./utility/*": {
      "import": "./utility/*.js",
      "require": "./dist/cjs/utility/*.js"
    },
    "./utility": {
      "import": "./utility/index.js",
      "require": "./dist/cjs/utility/index.js"
    },
    "./whisper-shell": {
      "import": "./whisper-shell.js",
      "require": "./dist/cjs/whisper-shell.js"
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "ffmpeg-static": "^5.2.0",
    "node-downloader-helper": "^2.1.4",
    "shelljs": "^0.8.5",
    "temp": "^0.9.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/shelljs": "^0.8.11",
    "@types/temp": "^0.9.3",
    "@types/uuid": "^9.0.6",
    "typescript": "4.9.5"
  },
  "main": "dist/cjs/index.js",
  "module": "index.js",
  "index": "dist/cjs/index.js"
}
