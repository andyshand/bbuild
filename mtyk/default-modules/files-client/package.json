{
  "name": "@bbuild-src/files-client",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./_deps": {
      "import": "./_deps.js",
      "require": "./dist/cjs/_deps.js"
    },
    "./component/*": {
      "import": "./component/*.js",
      "require": "./dist/cjs/component/*.js"
    },
    "./component": {
      "import": "./component/index.js",
      "require": "./dist/cjs/component/index.js"
    },
    "./hooks/*": {
      "import": "./hooks/*.js",
      "require": "./dist/cjs/hooks/*.js"
    },
    "./hooks": {
      "import": "./hooks/index.js",
      "require": "./dist/cjs/hooks/index.js"
    },
    "./index": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./rpc/*": {
      "import": "./rpc/*.js",
      "require": "./dist/cjs/rpc/*.js"
    },
    "./rpc": {
      "import": "./rpc/index.js",
      "require": "./dist/cjs/rpc/index.js"
    },
    "./utils/*": {
      "import": "./utils/*.js",
      "require": "./dist/cjs/utils/*.js"
    },
    "./utils": {
      "import": "./utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    }
  },
  "dependencies": {
    "@types/react": "^18.2.21",
    "buffer": "^6.0.3",
    "react": "18.2.0",
    "react-icons": "5.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/react": "^18.2.21",
    "typescript": "4.9.5"
  },
  "main": "dist/cjs/index.js",
  "module": "index.js",
  "index": "dist/cjs/index.js"
}
